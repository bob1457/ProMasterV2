@model ProMaster.Core.Reporting.ViewModels.AccountingCostViewModel

<div id="property">

<div class="title_icon">
    <img src="@Url.Content(@Model.PropertyImageUrl)" alt="" title="@Model.PropertyName" />
</div>
<!----> <div class="title_text">
    <h5>@Html.DisplayFor(model => model.PropertyName)</h5><br /><br />
    <span class="title_caption">@Html.DisplayFor(model => model.PropertyDesc)<br /></span>
</div>

<div id="addr">
    <br />
    @Html.DisplayFor(model=>model.PropertyAddressSuiteNumber),
    @Html.DisplayFor(model => model.PropertyAddressStreetNumber)
    @Html.DisplayFor(model => model.PropertyAddressStreetName)
    @Html.DisplayFor(model => model.PropertyAddressCity)
    @Html.DisplayFor(model => model.PropertyAddressProvState)
    @Html.DisplayFor(model => model.PropertyAddressPostZipCode)
    @Html.DisplayFor(model => model.PropertyAddressCountry)
    <br />
</div>



</div><br />
<p></p>
<div id="report_title">Property Financail Statement</div>
<div id="report_data">

    <!--Rent Revenue: &nbsp;-->
    <div id="revenue">
        <br /><br />
        <span class="total">Total Rent Revenue: $@ViewBag.totalRevenue</span>
        <p></p><br />
    </div>
    
    <span class="total">Less Total Expenses: $@ViewBag.totalExpenses</span>
    <br />
    
    Net Income: &nbsp;
    <span class="total">$@ViewBag.totalNetIncome</span>
    <p></p><br /><br /><br />

    
    <p><a class="left-aligned edit_button button_border" id="pdf" href="@Url.Content("~/Reporting/ExportReportPDF/?id=" + @ViewBag.id + "&tId=4&startDate=" + @ViewBag.StartDate + "&endDate=" + @ViewBag.EndDate)" target="_blank">Export in PDF</a></p>
</div>



<script type="text/javascript">
    $(document).ready(function () {
        //$("#revenue").hide();
        $("#r_cost").hide();
        $("#o_cost").hide();
        $("#m_cost").hide();
    });

</script>