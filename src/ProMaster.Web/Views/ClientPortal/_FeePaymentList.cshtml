@model ProMaster.Core.ClientPortal.ViewModels.DisplayClientPortalViewModel

<table>
    <thead class="table_head">
        <tr>
            <td colspan="6" class="table-h table_head">
                <span><i class="icon-clipboard-2"></i></span> Management Fee Payment
            </td>
        </tr>
    </thead>
    <tr>
        <th class="table_th">Month/Year</th>
        <th class="table_th">Payment Date</th>
        <th class="table_th">Payment Amount</th>
        <th class="table_th">Fee Type</th>
        <th class="table_th">Notes</th>
        <th class="table_th"></th>
    </tr>
    @if (@Model.FeePaymentHisotry != null)
    {
        foreach (var item in Model.FeePaymentHisotry)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FeeMonth)/@Html.DisplayFor(modelItem => item.FeeYear)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FeePaidDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FeePaidAmount)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FeeType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FeeNotes)
                </td>

                <td>
                    <div class="table-button"><a href="@Url.Action("FeePaymentDetails", "Manage", new { id = item.ContractId })"><img src="@Url.Content("~/Content/Images/details.png")" title="Details" /></a></div>
                </td>

            </tr>
        }

    }
    else
    {
        <tr>
            <td colspan="6">
                <em>No records!</em>
            </td>
        </tr>
    }
    <tfoot class="table_th">
        <tr>
            <td colspan="6"><span><i class="icon-clipboard-2"></i></span></td>

        </tr>
    </tfoot>

</table><br />